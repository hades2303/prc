import pandas as pe

D= { 'Toys" : [7916, 8508, 7226 2617],

'Babes [61896, 8208, 6149 6157), 'uni form: [ 610, 508, 611, 457],

"Shoes" (8810, 6798, 9611, 64573}

DF = pd.DataFrame(D, index=['AP','OD,'MP','UP'])

print ("Toys:!")

print(DF.loc[:, 'Toys']) 
print ("\n")

Print ("AP and OP")

print (DF.loc["AP":"OD"])

print ("\n")

print ("The details of MP, UP")

print (DF.loc['MP': 'Up', "Books": "uniform"])

print("\n")

print ("3rows, 3 Column")
print (DF.iloc[0:3, 0:3])


